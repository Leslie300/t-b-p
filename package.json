{
  "version": "0.0.2",
  "name": "tbp-app",
  "description": "tbp - frontend app logic",
  "homepage": "https://github.com/lhache/tbp-cms#readme",
  "main": "src/index.js",
  "bugs": {
    "url": "https://github.com/lhache/tbp-cms/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lhache/t-b-p.git"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "counterpart": "^0.18.1",
    "flexbox-react": "^4.4.0",
    "history": "^4.6.3",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "mobile-detect": "^1.3.6",
    "react": "^15.6.1",
    "react-cookie-banner": "0.0.18",
    "react-dom": "^15.6.1",
    "react-interpolate-component": "^0.11.0",
    "react-rater": "^0.4.3",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-select": "^1.0.0-rc.5",
    "react-svg": "^2.1.21",
    "react-translate-component": "^0.14.0",
    "react-truncate": "^2.1.5",
    "react-virtualized-select": "^3.1.0",
    "redux": "^3.7.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "expect": "^1.20.2",
    "fetch-mock": "^5.12.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "netlify-cli": "^1.2.2",
    "node-sass": "^4.5.3",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "react-scripts": "1.0.7",
    "redux-mock-store": "^1.2.3",
    "regenerator-runtime": "^0.10.5"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "test:watch": "npm test -- --watch",
    "deploy-surge": "surge --project ./build --domain t-b-p.surge.sh",
    "deploy-netlify": "netlify deploy"
  }
}
